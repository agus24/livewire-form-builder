    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Gustiawan\\LivewireFormBuilder\\LivewireFormBuilderServiceProvider"
            ]
        }
    }
}
