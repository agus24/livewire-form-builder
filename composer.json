{
    "name": "gustiawan/livewire-form-builder",
    "description": "Livewire Form Builder",
    "keywords": ["laravel", "form", "builder", "Livewire"],
    "type": "library",
    "authors": [
        {
            "name": "gustiawan",
            "email": "agusx244@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gustiawan\\LivewireFormBuilder\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0",
        "illuminate/validation": "^9.0",
